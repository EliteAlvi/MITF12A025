@model TheEliteCars.Models.User
@{
    ViewBag.Title = "EditUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>EditUser</h2>
<form action="/Cars/EditConfirm/@Model.Role" method="post">
    Name <input type="text" value="@Model.Name" name="t1" />
    Password <input type="text" value="@Model.Password" name="t2" />
    <input type="submit" value="Update Car" />
</form>